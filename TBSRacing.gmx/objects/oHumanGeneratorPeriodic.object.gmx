            gener = myid;
            humanStartRandomMadePath(pp, cr);
            path_shift(path, irandom(xs) * choose(-1, 1), irandom(ys) * choose(-1, 1));
            visible = 1;    
            spd *= coeff;
            handLSpd *= coeff;
            handRSpd *= coeff;
            legLSpd *= coeff;
            legRSpd *= coeff;
        }
    }    
}
