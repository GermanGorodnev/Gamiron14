            <string>/// update
if (!(flags &amp; OBJ_PAR.UPDATABLE))
    exit;
    
if (tmr &amp;&amp; !--tmr)
{
    tmr = irandom_range(tmrMin, tmrMax);
    if ((maxPeopleCnt != -1)
        &amp;&amp; (peopleCnt &gt;= maxPeopleCnt))
        exit;
    if (humanCountOfMe(id) &gt; maxpeople)
    {
        exit;
    }

    var pp = path, cr = canreverse,
        xs = xshiftmax, ys = yshiftmax,
        myid = id;
    repeat(irandom_range(peoplemin, peoplemax))
    {
        peopleCnt++;
        var coeff = random_range(speedcoeffmin, speedcoeffmax);
        with (instance_create(0, 0, oHuman))
