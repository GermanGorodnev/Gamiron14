        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (!(flags &amp; OBJ_PAR.UPDATABLE))
{
    speed = 0;
    exit;
}
    
if (camFlags &amp; CAM_FLAGS.SHAKING)
{
    if (shakeTimer &amp;&amp; !--shakeTimer)
    {
        shakeTimer = -1;
        camFlags = flagSetVar(camFlags, CAM_FLAGS.SHAKING, 0);
        shX = 0;
        shY = 0;
    }   
}

if (camFlags &amp; CAM_FLAGS.DARKEN)
{
    if (darkenA &lt; darkenNeed)
        darkenA = min(darkenA + 0.03, darkenNeed);
    if (darkenA &gt; darkenNeed)
        darkenA = max(darkenA - 0.03, darkenNeed);
}

switch (state)
{
case CAM_STATES.__FREE:
    var ymove = oControl.key[KEY.CAM_BOTTOM] - oControl.key[KEY.CAM_TOP],
        xmove = oControl.key[KEY.CAM_RIGHT] - oControl.key[KEY.CAM_LEFT],
        spdmove = movSpd;
    x += spdmove * xmove;
    y += spdmove * ymove;
    x = clamp(x, view_wview div 2, room_width - view_wview div 2);
    y = clamp(y, view_hview div 2, room_height - view_hview div 2);
    break;
    
case CAM_STATES.__FREE_WITH_MOUSE:
    var ymove = oControl.key[KEY.CAM_BOTTOM] - oControl.key[KEY.CAM_TOP],
        xmove = oControl.key[KEY.CAM_RIGHT] - oControl.key[KEY.CAM_LEFT],
        spdmove = movSpd;
    if (mouse_wheel_down())
    {
        if (ymove == 0)
            spdmove *= 2;
        ymove = 1;
    }
    else if (mouse_wheel_up())
    {
        if (ymove == 0)
            spdmove *= 2;
        ymove = -1;
    }
    x += spdmove * xmove;
    y += spdmove * ymove;
    x = clamp(x, view_wview div 2, room_width - view_wview div 2);
    y = clamp(y, view_hview div 2, room_height - view_hview div 2);
    break;
    
case CAM_STATES.__PARTICULAR_OBJ:
    if (oControl.key[KEY.CAM_TOP] == 1
        || oControl.key[KEY.CAM_BOTTOM] == 1
        || oControl.key[KEY.CAM_RIGHT] == 1
        || oControl.key[KEY.CAM_LEFT] == 1)
    {
        camSetState(CAM_STATES.__FREE);
        speed = 0;
        exit;
    }
    var dist = distance_to_point(targX, targY);
    if (dist &lt; movSpd)
    {
        speed = 0;
        x = targX;
        y = targY;
        camSetState(CAM_STATES.__FREE);
    }
    else
    {
        var dd = point_direction(x, y, targX, targY);
        x += lengthdir_x(max(dist / 8, movSpd), dd);   
        y += lengthdir_y(max(dist / 8, movSpd), dd);  
    }
    x = clamp(x, view_wview div 2, room_width - view_wview div 2);
    y = clamp(y, view_hview div 2, room_height - view_hview div 2);
    break;
}

</string>
