{
case 0:
    st0perc= min(1, st0perc + .05);
    if (oControl.key[KEY.MOUSE_LEFT] == 0)
    {
        if (mouseOnUs())
        {
            if (oGarageController.dragInd != -1)
            {
                // set the car
                garageCarSetCar(oGarageController.dragInd);
                with (oGarageController)
                {
                    dragInd = -1;
                    garageChangeFolder(folderAInd);
                    garageUpdateCarSlots();
                }
                with (oControl)
                    gameSave();
            }
            else
            {
                // go to detail mode
                var myind = ind;
                with (oGarageController)
                    garageStateInit(1, myind);   
            }
        }
    }
    break;
case 1: // edit
    // move to the center
    st1perc = min(1, st1perc + .03);
    st0x1 += max(min(abs(xcenter - st0x1), 5), abs(xcenter - st0x1) / 5) * sign(xcenter - st0x1);
    spx = st0x1 + st0w / 2;
    break;
    
case 2: // fadeaway
    st0perc= max(0, st0perc - .05);
    break;
    
}
